{
	"compilerOptions": {
		"module": "commonjs",
		"target": "esnext",
		"lib": [
			"es2015",
			"webworker"
		],
		"jsx": "react",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"outDir": "./dist",
		"moduleResolution": "node"
  	},
	"exclude": [
		"./node_modules",
		"./dist"
	],
	"include": [
		"./src/*.ts",
		"./src/**/*.ts",
	],
}